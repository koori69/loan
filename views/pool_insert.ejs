<form class="form-horizontal" method="post">
  <fieldset>
    <legend>注入资金</legend>
    <div class="control-group">
      <label class="control-label" for="funder">注资人</label>
      <div class="controls">
        <input type="text" class="input-xlarge" id="funder" name="funder">
      </div>
    </div>
    <div class="control-group">
      <label class="control-label" for="amount">注入金额</label>
      <div class="controls">
        <input type="number" class="input-xlarge" id="amount" name="amount">
      </div>
    </div>
    <div class="control-group">
      <label class="control-label" for="in_date">注资日期</label>
      <div class="controls">
        <input type="date" class="input-xlarge" id="in_date" name="in_date">
      </div>
    </div>
    <div class="control-group">
      <label class="control-label" for="out_reserved">准备抽出</label>
      <div class="controls">
          <select class="input-xlarge" id="out_reserved" name="out_reserved">
              <option>是</option>
              <option>否</option>
          </select>
      </div>
    </div>
    <div class="control-group">
      <label class="control-label" for="out_date">抽出日期</label>
      <div class="controls">
        <input type="date" class="input-xlarge" id="out_date" name="out_date">
      </div>
    </div>
    <div class="form-actions">
      <button type="submit" class="btn btn-primary">注入</button>
    </div>
  </fieldset>
</form>